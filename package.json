    "sass": "^1.56.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.4",
    "vue": "^2.7.14",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.9"
