    "vue-i18n": "^8",
    "vue-select": "^3.20.0",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
